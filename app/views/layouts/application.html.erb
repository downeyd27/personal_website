<!DOCTYPE html>
<html>
<head>
  <title>Dillon Downey's Personal Website</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <%= csrf_meta_tags %>
</head>

<body>
  <h1 class="sr-only">Dillon Downey's Web Portfolio</h1>
  <!-- ################################# -->
  <!-- ###########  NAVBAR ############# -->
  <%= render 'partials/navbar_top' %>
  <!-- ###########  NAVBAR ############## -->
  <!-- ################################## -->
  <main>
    <div class="flex justify-content-center">
      <h1 class="debug "><span translate="no">Dillon Downey</span> - Junior Web Developer</h1>
    </div>
    <!-- ################################# -->
    <!-- ######### INTRO SECTION ######### -->
    <%= render 'partials/intro_section' %>
    <!-- ######### INTRO SECTION ######### -->
    <!-- ################################# -->

    <!-- ################################# -->
    <!-- ######## PROJECTS SECTION ####### -->
    <%= render 'partials/projects_section' %>
    <!-- ######## PROJECTS SECTION ####### -->
    <!-- ################################# -->

    <!-- ################################# -->
    <!-- ############# YIELD ############# -->
    <%# yield %>
    <!-- ################################# -->
    <!-- ############# YIELD ############# -->
  </main>

  <!-- ################################# -->
  <!-- ############ FOOTER ############# -->
  <%= render 'partials/footer.html' %>
  <!-- ############ FOOTER ############# -->
  <!-- ################################# -->

  <!-- At bottom of page so page will load faster -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js" defer></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js" defer></script>
</body>
</html>
